{
    "name": "Nausea addict",
    "version": "0.1",
    "manifest_version": 3,
    "author": "Dimitri Timoz",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "main.js"
            ]
        }
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "storage"
    ],
    "host_permissions": [
        "*://*/*"
    ],
    "icons": {
        "16": "assets/logo16.png",
        "48": "assets/logo48.png",
        "128": "assets/logo128.png"
    }
}